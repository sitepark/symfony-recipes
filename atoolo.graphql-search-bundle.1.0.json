{
    "manifests": {
        "atoolo/graphql-search-bundle": {
            "manifest": {
                "bundles": {
                    "Atoolo\\GraphQL\\Search\\AtooloGraphQLSearchBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                }
            },
            "files": {
                "config/packages/graphql.yaml": {
                    "contents": [
                        "overblog_graphql:",
                        "  definitions:",
                        "    schema:",
                        "      query: RootQuery",
                        "      mutation: RootMutation",
                        "    mappings:",
                        "      types:",
                        "        - types: [yaml, graphql]",
                        "          dir: \"%kernel.project_dir%/vendor/atoolo/graphql-search-bundle/src/Resources/schema/defaults\"",
                        "          suffix: ~",
                        ""
                    ],
                    "executable": false
                },
                "config/routes/graphql.yaml": {
                    "contents": [
                        "overblog_graphql_endpoint:",
                        "    resource: \"@OverblogGraphQLBundle/Resources/config/routing/graphql.yml\"",
                        "    prefix: /api/graphql",
                        "",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "97dcf9103b718ef7adfe5b1752c7b691efdb7c88"
        }
    }
}
